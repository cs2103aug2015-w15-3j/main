                     <font>
                        <Font name="Droid Sans Mono" size="18.0" />
                     </font>
                  </Button>
                  <Label fx:id="numOfNextTasks" alignment="BASELINE_LEFT" prefHeight="33.0" prefWidth="45.0" style="-fx-font-weight: bold; -fx-font-size: 16;" text="0" textFill="#797d7f" translateX="38.0">
                     <font>
                        <Font name="Droid Sans Georgian" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
